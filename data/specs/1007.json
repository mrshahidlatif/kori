{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {"url": "data/seattle-weather.csv"},
  "mark": "bar",
  "encoding": {
    "x": {"timeUnit": "month", "field": "date", "title": "Date"},
    "y": {
      "aggregate": "mean",
      "field": "precipitation",
      "title": "Mean of precipitation"
    }
  },
  "config": {}
}