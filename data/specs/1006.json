{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {"url": "data/seattle-weather.csv"},
  "mark": {"type": "line", "interpolate": "monotone"},
  "encoding": {
    "x": {"timeUnit": "month", "field": "date", "title": "Date"},
    "y": {
      "aggregate": "mean",
      "field": "temp_max",
      "title": "Mean of Maximum Temperature"
    }
  },
  "config": {}
}